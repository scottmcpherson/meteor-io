{
  "commands": {
    "create": {
      "packages": {
        "add": [
          "kadira:flow-router",
          "react-meteor-data"
        ],
        "remove": [
          "autopublish",
          "insecure"
        ]
      },
      "npm": {
        "install": [
          "meteor-node-stubs",
          "react-mounter",
          "react",
          "react-dom",
          "react-addons-pure-render-mixin"
        ]
      }
    }
  },
  "generators": {
    "template": {
      "dest": "$APP_PATH/client/templates/$DEST_PATH"
    },
    "route": {
      "dest": "$APP_PATH/imports/startup/client/routes.js"
    },
    "component": {
      "dest": "$APP_PATH/imports/ui/components/$DEST_PATH"
    },
    "page": {
      "dest": "$APP_PATH/imports/ui/pages"
    }
  }
}
